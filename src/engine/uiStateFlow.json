{
  "uiStateFlow": {
    "states": {
      "HomeScreen": {
        "transitions": {
          "startCampaign": "LevelSelectScreen",
          "startEndless": "EndlessModeScreen",
          "openSettings": "SettingsScreen"
        }
      },
      "LevelSelectScreen": {
        "transitions": {
          "selectLevel": "ChallengeScreen",
          "back": "HomeScreen"
        }
      },
      "ChallengeScreen": {
        "requiresGeneratedChallenge": true,
        "transitions": {
          "applyFormatting": "ChallengeScreen",
          "viewQuestion": "QuestionScreen",
          "back": "LevelSelectScreen"
        }
      },
      "QuestionScreen": {
        "requiresGeneratedChallenge": true,
        "transitions": {
          "submitCorrect": "LevelCompleteScreen",
          "submitIncorrect": "QuestionScreen",
          "backToDataset": "ChallengeScreen"
        }
      },
      "LevelCompleteScreen": {
        "transitions": {
          "nextLevel": "ChallengeScreen",
          "levelSelect": "LevelSelectScreen",
          "home": "HomeScreen"
        }
      },
      "EndlessModeScreen": {
        "transitions": {
          "startRun": "ChallengeScreen",
          "continueRun": "ChallengeScreen",
          "back": "HomeScreen"
        }
      },
      "SettingsScreen": {
        "transitions": {
          "back": "HomeScreen"
        }
      }
    }
  }
}